{# Setup variables -#}
{%- set koc = ('%.7f' % adsorption.koc).rstrip('0').rstrip('.') -%}
{% if adsorption.limit_freundl == 0 -%}
    {% set limit_freundlich = "0.00E+00" -%}
{% else -%}
    {% set limit_freundlich = "%1.0E" % adsorption.limit_freundl -%}
{% endif -%}
{# Write line -#}
{{ ' ' -}}{{ koc -}}
{{ '         ' -}}{{adsorption.freundlich -}}
{{ '       ' -}}{{adsorption.pH -}}
{{ '          '}}{{adsorption.pKa -}}
{{ '         ' }}{{ limit_freundlich -}}
{{ '          ' }}{{adsorption.annual_decrease_of_sorption_percent -}}
{{ '      ' }}{{adsorption.k_doc -}}
{{ '     ' }}{{ adsorption.increase_of_sorption_when_air_dried -}}
{{'    '}}{{adsorption.koc2 -}}
{{'    '}}{{adsorption.pH2 -}}
{{'     '}}{{adsorption.fraction_non_equilibrium_domain -}}
{{'     '}}{{adsorption.desorption_at_non_equilibrium_sites -}}
{{' \n' -}}