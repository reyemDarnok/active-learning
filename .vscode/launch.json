{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Short Infer Run",
            "type": "debugpy",
            "request": "launch",
            "program": "focusStepsPelmo/pelmo/local.py",
            "console": "integratedTerminal",
            "args": "-c infer_short.json -g examples/gap.json -o new_ppdb_inferred.csv"
        },
        {
            "name": "Pelmo Inferred Run",
            "type": "debugpy",
            "request": "launch",
            "program": "focusStepsPelmo/pelmo/local.py",
            "console": "integratedTerminal",
            "args": "-c ppdb_source/inferred2pelmo.json -g examples/gap.json -o new_ppdb_inferred.csv"
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Local Single Run",
            "type": "debugpy",
            "request": "launch",
            "program": "code/focusStepsPelmo/pelmo/local.py",
            "console": "integratedTerminal",
            "args": "-c examples/compound.json -c examples/compound.json -g examples/gap.json -w test/localtest -o test/localout.json --crop AP -s C --loglevel DEBUG"
        },
        {
            "name": "Single Scan",
            "type": "debugpy",
            "request": "launch",
            "program": "code/focusStepsPelmo/pelmo/scan.py",
            "console": "integratedTerminal",
            "args": "-c examples/compound.json -g examples/gap.json -w test/minisample --crop AP --scenario C -o test/minisamp.csv -i micro-scan.json -s 1 --loglevel DEBUG local"
        },
        {
            "name": "Microscan",
            "type": "debugpy",
            "request": "launch",
            "program": "code/focusStepsPelmo/pelmo/scan.py",
            "console": "integratedTerminal",
            "args": "-c examples/compound.json -g examples/gap.json -w test/minisample -o test/minisamp.csv -i micro-scan.json -s 5 --loglevel DEBUG local"
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        }
    ]
}